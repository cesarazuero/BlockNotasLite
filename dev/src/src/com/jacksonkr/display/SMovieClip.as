package com.jacksonkr.display {	import com.jacksonkr.events.FrameChangeEvent;	import flash.display.MovieClip;	import flash.events.Event;		public class SMovieClip extends MovieClip {		protected var _last_frame_name:String;				public function SMovieClip() {			super();						this._last_frame_name = this.currentFrameLabel;						this.addEventListener(Event.ENTER_FRAME, this.enterFrameHandler);		}				protected function enterFrameHandler(event:Event):void {			if(this.currentLabel != this._last_frame_name) {				var label:String = this.currentLabel;				this._last_frame_name = label; // set this equal to this.currentFrame to listen for any frame changes instead.				dispatchEvent(new FrameChangeEvent(FrameChangeEvent.FRAME_CHANGE, label));			}		}	}}